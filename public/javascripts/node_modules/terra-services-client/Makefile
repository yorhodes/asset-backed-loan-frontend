run:
	rm -rf build/*
	make/transpile.sh
	coffee -o build/temp -c ./tester.coffee
	node build/temp/tester.js

deploy:
	rm -rf build/*
	#make/transpile.sh
	coffee -o build/temp/lib -c lib/equery.coffee
	coffee -o build/temp/lib -c lib/rsa.coffee
	coffee -o build/temp/lib -c lib/key.coffee
	coffee -o build/temp/lib -c lib/store.coffee
	coffee -o build/temp/contracts -c contracts/router.coffee
	coffee -o build/temp/contracts -c contracts/push.coffee
	coffee -o build/temp/contracts -c contracts/usd.coffee
	coffee -o build/temp/contracts -c contracts/accounts.coffee
	coffee -o build/temp/contracts -c contracts/loan.coffee

	coffee -o build/temp -c deploy.coffee
	coffee -o build/temp -c core.coffee

	make/compile.sh
	rm -rf build/temp/*
	rm -r build/temp
